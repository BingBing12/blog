
<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newPost">Title</label>
        <input class="form-control" type="text" name="postTitle" id="newPost">
        <label for="newText">Post</label>
        <textarea class="form-control" name="postBody" id="newText" cols="30" rows="10"></textarea>
        <button class="btn btn-lg btn-primary btnCompose" type="submit">Publish</button>
    </div>
</form>
<%- include("partials/footer") -%>